{
    "name": "chubbyphp/chubbyphp-session-storageless",
    "description": "Storageless persistence adapter for zend-expressive-session",
    "keywords": ["chubbyphp", "session", "zend-expressive"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Zogg",
            "email": "dominik.zogg@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "zendframework/zend-expressive-session": "^1.2",
        "psr7-sessions/storageless": "^5.0"
    },
    "require-dev": {
        "chubbyphp/chubbyphp-mock": "^1.4",
        "phpunit/phpunit": "^8.1",
        "phploc/phploc": "^5.0",
        "zendframework/zend-stratigility": "^3.1",
        "zendframework/zend-diactoros": "^2.1"
    },
    "autoload": {
        "psr-4": { "Chubbyphp\\Session\\Storageless\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
